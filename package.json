{
  "name": "@janus/janus.js",
  "version": "0.0.1",
  "scripts": {
    "build:dev": "node build.mjs",
    "build:prod": "NODE_ENV=production node build.mjs",
    "build:tests": "node build.tests.mjs",
    "build:clean": "echo \" Not implemented yet! \"",
    "dev:start": "docker compose -f infra/dev.compose.yml up --build",
    "dev:watch": "docker compose -f infra/dev.compose.yml -f infra/build.compose.yml up --build",
    "dev:test": "echo \" Not implemented yet! \"",
    "dev:clean": "docker compose -f infra/dev.compose.yml -f infra/build.compose.yml down --volumes --remove-orphans",
    "lint:setup": "rome start",
    "lint:format": "rome format service tests libs --write --use-server",
    "lint:check": "rome check service tests libs --use-server",
    "lint:apply": "rome check service tests libs --apply-unsafe --use-server",
    "lint:fix": "yarn lint:format && yarn lint:apply",
    "test:lint": "rome ci svcs tests",
    "test:tsc": "tsc --noEmit",
    "test:unit": "echo \" Not implemented yet! \"",
    "test:e2e": "echo \" Not implemented yet! \"",
    "test:watch": "echo \" Not implemented yet! \"",
    "test:prod": "echo \" Not implemented yet! \"",
    "test:all": "echo \" Not implemented yet! \""
  },
  "workspaces": [
    "dist/*",
    "libs/*",
    "providers/*",
    "routes/*",
    "service"
  ],
  "devDependencies": {
    "@libs/build": "workspace:*",
    "@types/node": "20.1.2",
    "rome": "12.0.0",
    "typescript": "5.0.4"
  },
  "type": "module",
  "packageManager": "yarn@4.0.0-rc.43"
}
